<?xml version="1.0" encoding="UTF-8"?>
<phpunit>
    <testsuites>
        <testsuite name="PHP_CodeSniffer Test Suite">
            <file>tests/AllTests.php</file>
        </testsuite>
    </testsuites>
    <filter>
        <whitelist>
            <directory>./CodeSniffer</directory>
            <exclude>
                <directory>./CodeSniffer/Standards/*/Tests</directory>
            </exclude>
        </whitelist>
    </filter>
</phpunit>
