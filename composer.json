        "reference": "https://github.com/squizlabs/PHP_CodeSniffer/zipball"
    },
    "suggest": {
        "sebastianbergmann/php-timer": "Allows more precise runtime tracking"
    },
    "autoload": {
        "psr-0": {"PHP_CodeSniffer": "CodeSniffer/"}
    },
    "bin": ["scripts/phpcs"]
