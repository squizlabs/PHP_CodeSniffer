    "homepage": "http://github.com/squizlabs/PHP_CodeSniffer",
    "license": "BSD-3-Clause",
    "version": "1.3.4",
    "dist": {
        "type": "zip",
        "url":  "https://github.com/squizlabs/PHP_CodeSniffer/zipball/1.3.4"
    },
    "require": {
        "php": ">=5.1.2"
